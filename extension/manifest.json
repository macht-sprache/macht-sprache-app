{
    "name": "macht.sprache.",
    "description": "automatically find sensitive translations",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["https://translate.google.com/*"],
            "css": ["./content-style.css"],
            "js": ["./content-script.js"]
        }
    ],
    "web_accessible_resources": [
        {
            "matches": ["https://translate.google.com/*"],
            "resources": ["images/*"]
        }
    ]
}
